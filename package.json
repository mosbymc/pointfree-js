{
  "name": "pointfree-js",
  "version": "0.0.1",
  "description": "A JavaScript library that bring together many aspects of functional programming into a single environment",
  "main": "index.js",
  "scripts": {
    "babel": "babel src --out-dir src && npm version patch",
    "build": "npm run lint && npm test && npm run bundle && npm run minify",
    "build:full": "npm run build && npm run documentation",
    "prebuild:dev": "del-cli dev_browser/pointfree.js",
    "build:dev": "npm run lint && npm test && npm run bundle:dev",
    "prebundle": "del-cli dist/*",
    "bundle": "./node_modules/.bin/browserify index.js -t [ babelify --presets [es2015] --plugins [ transform-es2015-modules-commonjs babel-polyfill ] --only src ] -o dist/pointfree.js",
    "bundle:dev": "./node_modules/.bin/browserify index.js -t [ babelify --presets [es2015] --plugins [ transform-es2015-modules-commonjs babel-polyfill --sourcemaps true ] --only src ] -d -o dev_browser/pointfree.js",
    "bundle:minify": "npm run bundle && npm run minify",
    "clean": "npm run clean:build && npm run clean:coverage && npm run clean:dev && npm run clean:plato",
    "clean:build": "del-cli dist/*",
    "clean:coverage": "del-cli coverage/*",
    "clean:dev_browser": "del-cli dev_browser/*",
    "clean:plato": "del-cli plato/*",
    "codeclimate:coverage": "cross-env CODECLIMATE_REPO_TOKEN=88f6338a30b9b3b63e26685f78c813c4ca0448d6e21b7f2fed6878ccc11e7f47 codeclimate-test-reporter < ./coverage/lcov.info",
    "precoverage": "del-cli coverage/*",
    "coverage": "cross-env NODE_ENV=test nyc mocha test/unit/**/*.spec.js",
    "coverage:report": "nyc report --reporter=text-lcov",
    "predocumentation": "del-cli documentation/*",
    "documentation": "./node_modules/.bin/jsdoc -r src -d documentation -R ./README.md -c jsdoc.conf.json",
    "eslint": "./node_modules/.bin/eslint src",
    "jshint": "./node_modules/.bin/jshint src",
    "lint": "npm run jshint && npm run eslint",
    "lint:test": "npm run lint && npm test",
    "lint:test:coverage": "npm run lint && npm run coverage",
    "minify": "uglifyjs dist/pointfree.js -o dist/pointfree.min.js -c -m --source-map",
    "preplato": "del-cli plato",
    "plato": "plato -r -d plato -l .jshintrc -e .eslintrc.json -t 'pointfree-js' -n src",
    "snyk": "./node_modules/.bin/snyk test",
    "start": "node app.js 3000",
    "test": "cross-env NODE_ENV=test mocha test/unit/**/*.spec.js --require babel-polyfill --require test/test_helper --compilers js:babel-register"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "tempDirectory": "./coverage/nyc_raw",
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "require": [
      "babel-polyfill",
      "babel-register",
      "./test/test_helper.js"
    ],
    "exclude": [
      "./src/streams/source.js",
      "./src/steams/stream.js",
      "./src/streams/stream_test_files.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mosbymc/queryable-js"
  },
  "keywords": [
    "combinator",
    "decorator",
    "transducer",
    "lens",
    "monad",
    "functor",
    "functional",
    "functional programming",
    "fp",
    "functional javascript",
    "stream"
  ],
  "license": "MIT",
  "author": "Mark Mosby",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.21.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.14.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.17.2",
    "brfs": "^1.4.3",
    "browser-sync": "^2.11.1",
    "browserify": "^14.4.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.5.0",
    "cross-env": "^5.0.5",
    "del": "^3.0.0",
    "del-cli": "^1.1.0",
    "docdash": "^0.4.0",
    "eslint": "^4.4.1",
    "express": "^4.15.4",
    "google-closure-compiler-js": "^20170806.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-closure-compiler": "^0.4.0",
    "gulp-filter": "^5.0.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-plumber": "^1.1.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-strip-comments": "^2.5.1",
    "gulp-task-listing": "^1.0.1",
    "gulp-util": "^3.0.7",
    "jsdoc": "^3.5.4",
    "jshint": "^2.9.1",
    "mocha": "^3.2.0",
    "mocha-sinon": "^2.0.0",
    "nyc": "^11.1.0",
    "plato": "^1.5.0",
    "proxyquire": "^1.7.4",
    "proxyquireify": "^3.2.1",
    "rootpath": "^0.1.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.13.0",
    "snyk": "^1.38.2",
    "source-map-support": "^0.4.15",
    "uglify-js": "^3.0.27",
    "wallabify": "0.0.15",
    "yargs": "^8.0.2"
  }
}

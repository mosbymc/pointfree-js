{
  "name": "queryable-js",
  "version": "0.0.1",
  "description": "A JavaScript library for performing queries against lists with lazy evaluation and deferred execution.",
  "main": "app.js",
  "scripts": {
    "yuidoc": "yuidoc",
    "babs": "babel --version",
    "babel": "babel src --out-dir src && npm version patch",
    "build": "babel --out-dir dist",
    "test": "set NODE_ENV=test&& mocha test/unit/**/*.spec.js --require babel-polyfill test/test_helper --compilers js:babel-register",
    "karma_test": "karma start karma.conf.js",
    "clean": "npm run clean:build && npm run clean:test && npm run clean:plato",
    "clean:build": "npm run del build",
    "clean:test": "npm run del coverage",
    "clean:plato": "npm run del plato",
    "lint": "npm run lint && npm run jscs",
    "coverage": "nyc npm test"
  },
  "nyc": {
    "sourceMap": "inline",
    "instrument": true,
    "reporter": [ "text", "text-summary", "html", "lcov"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mosbymc/queryable-js"
  },
  "license": "MIT",
  "author": "Mark Mosby",

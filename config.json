{
  "reporterEnabled": "spec, list, doc",
  "specReporterOptions": {
    "output": "reports/spec.txt"
  },
  "listReporterOptions": {
    "output": "reports/list.txt"
  },
  "mochaLcovReporterReporterOptions": {
    "output": "reports/coverage.lcov"
  },
  "docReporter": {
    "output": "reports/doc.txt",
    "mochaFile": "reports/doc.txt"
  }
}